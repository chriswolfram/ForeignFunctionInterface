Cell[BoxData[
 RowBox[{
  RowBox[{"readStrings", "[", 
   RowBox[{"array_", ",", "len_"}], "]"}], ":=", 
  RowBox[{"Table", "[", 
   RowBox[{
    RowBox[{"With", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"ptr", "=", 
        RowBox[{"DereferenceBuffer", "[", 
         RowBox[{"array", ",", "\"\<OpaqueRawPointer\>\"", ",", 
          RowBox[{"i", "-", "1"}]}], "]"}]}], "}"}], ",", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"ptr", "=!=", 
         RowBox[{"OpaqueRawPointer", "[", "0", "]"}]}], ",", 
        RowBox[{"BufferToString", "[", "ptr", "]"}], ",", "Null"}], "]"}]}], 
     "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"i", ",", "len"}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.883586964867652*^9, 3.8835869873077307`*^9}},
 CellLabel->"In[78]:=",
 CellID->2025003536,ExpressionUUID->"3ff4a59f-2a15-45d5-9182-74a72636b335"],

