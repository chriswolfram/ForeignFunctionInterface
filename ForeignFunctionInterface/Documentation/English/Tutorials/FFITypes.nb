 CellID->480864139,ExpressionUUID->"96865f86-01c6-412d-81b8-15b48b29f60a"],

Cell[CellGroupData[{

Cell["\<\
Structs can be represented with the \"ListTuple\" parameterized type, or with \
lists. For example, the following struct:\
\>", "Text",
 CellChangeTimes->{{3.8805497775993032`*^9, 3.880549799431666*^9}, {
  3.880549879254958*^9, 3.880549911926888*^9}},
 CellID->210299655,ExpressionUUID->"ad12cdd1-878d-4204-a60d-5c67a3c3e8ff"],

Cell["\<\
typedef struct {
\tint a;
\tlong b;
\tdouble c;
} my_struct;\
\>", "Program",
 CellChangeTimes->{{3.8805499462924337`*^9, 3.880549962872539*^9}},
 CellID->878360205,ExpressionUUID->"b550278b-541d-458a-a7c5-05ce966d5f99"]
}, Open  ]],

Cell[CellGroupData[{

Cell["could be represented with:", "Text",
 CellChangeTimes->{{3.880549917930387*^9, 3.880550012343289*^9}},
 CellID->1116347675,ExpressionUUID->"5d6ed004-7f65-4a57-bae7-e1923c48389e"],

Cell[BoxData[
 RowBox[{"\"\<ListTuple\>\"", "::[", 
  RowBox[{"\"\<CInt\>\"", ",", "\"\<CLong\>\"", ",", "\"\<CDouble\>\""}], 
  "]"}]], "Input",
 CellChangeTimes->{3.880550005961205*^9},
 CellID->1270821179,ExpressionUUID->"2c04534f-5941-4c8e-ae92-952e852ccfbe"]
}, Open  ]],

Cell[CellGroupData[{

Cell["or with:", "Text",
 CellChangeTimes->{{3.88055001592483*^9, 3.880550016932846*^9}},
 CellID->1959214392,ExpressionUUID->"02976253-0bdb-45a6-8139-f7558a110ffa"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"\"\<CInt\>\"", ",", "\"\<CLong\>\"", ",", "\"\<CDouble\>\""}], 
  "}"}]], "Input",
 CellChangeTimes->{{3.880549998545775*^9, 3.880550001729411*^9}},
 CellID->1718402450,ExpressionUUID->"1e2b7c97-8552-4017-ba8d-a6d558e2aca2"]
}, Open  ]],

Cell["\<\
All that matters is that the correct types are in the correct order. \
Information about the names of the fields is ignored.\
\>", "Text",
 CellChangeTimes->{{3.880550023836279*^9, 3.880550058384231*^9}},
 CellID->861311097,ExpressionUUID->"120d0d21-091c-47b1-87ea-432f69ee022b"],

Cell["\<\
Structs returned from external programs are represented with lists.\
\>", "Text",
